{
    "name": "marcinmisiak/yii2-firebird",
    "description": "Firebird 3 + php8.1",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","firebird","php81"],
    "license": "GPL-3.0+",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/edgardmessias/yii2-firebird/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/edgardmessias/yii2-firebird"
    },
    "authors": [
        {
            "name": "Marcin Misiak",
            "email": "2mmarcinmisiak@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "marcinmisiak\\db\\firebird\\": "src"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            },
            "vcs-driver-options": {
                "github-no-api": true
            },
            "pattern-skip-version": "(-build|-patch)"
        }
    }
}
